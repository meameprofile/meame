  "scripts": {
    "//": "--- [1] CICLO DE VIDA BASE ---",
    "dev": "pnpm build:i18n && cross-env NODE_OPTIONS='--dns-result-order=ipv4first' next dev",
    "build": "pnpm validate && pnpm build:i18n && next build",
    "start": "next start",
    "lint": "eslint .",
    "//": "--- [2] CALIDAD Y VALIDACIÓN ---",
    "format:write": "prettier --write . --log-level warn",
    "format:check": "prettier --check . --log-level warn",
    "check-types": "tsc --noEmit",
    "validate:themes": "tsx scripts/validation/validate-theme-fragments.ts",
    "validate:sdc": "tsx scripts/validation/validate-sdc-config.ts",
    "validate": "pnpm lint && pnpm format:check && pnpm check-types && pnpm validate:themes && pnpm validate:sdc",
    "//": "--- [3] GENERACIÓN AUTOMÁTICA DE CÓDIGO ---",
    "build:i18n": "tsx scripts/generation/build-i18n-dictionaries.ts",
    "gen:icons": "tsx scripts/generation/generate-lucide-icon-enum.ts",
    "gen:routes": "tsx scripts/generation/generate-navigation-manifest.ts",
    "generate": "pnpm gen:icons && pnpm gen:routes",
    "//": "--- [4] DIAGNÓSTICOS DE SUPABASE ---",
    "diag:supabase:connect": "tsx scripts/supabase/connect.ts",
    "diag:supabase:schema-all": "tsx scripts/run-with-env.ts scripts/supabase/schema-all.ts",
    "diag:supabase:schema-profiles": "tsx scripts/run-with-env.ts scripts/supabase/schema-profiles.ts",
