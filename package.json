    "diag:supabase:schema-cogniread": "tsx scripts/run-with-env.ts scripts/supabase/schema-cogniread_articles.ts",
    "diag:supabase:schema-razprompts": "tsx scripts/run-with-env.ts scripts/supabase/schema-razprompts_entries.ts",
    "diag:supabase:schema-functions": "tsx scripts/run-with-env.ts scripts/supabase/schema-functions.ts",
    "diag:supabase:schema-triggers": "tsx scripts/run-with-env.ts scripts/supabase/schema-triggers.ts",
    "diag:supabase:schema-rls": "tsx scripts/run-with-env.ts scripts/supabase/schema-rls.ts",
    "diag:supabase:content-all": "tsx scripts/run-with-env.ts scripts/supabase/content-all.ts",
    "diag:supabase:content-profiles": "tsx scripts/run-with-env.ts scripts/supabase/content-profiles.ts",
    "diag:supabase:all": "pnpm diag:supabase:connect && pnpm diag:supabase:schema-all && pnpm diag:supabase:content-all",
    "//": "--- [5] DIAGNÓSTICOS DE CLOUDINARY ---",
    "diag:cloudinary:connect": "tsx scripts/cloudinary/connect.ts",
    "diag:cloudinary:schema": "tsx scripts/run-with-env.ts scripts/cloudinary/schema.ts",
    "diag:cloudinary:content": "tsx scripts/run-with-env.ts scripts/cloudinary/content.ts",
    "diag:cloudinary:all": "pnpm diag:cloudinary:connect && pnpm diag:cloudinary:schema && pnpm diag:cloudinary:content",
    "//": "--- [6] DIAGNÓSTICOS DE MONGODB (LEGACY) ---",
    "diag:mongo:connect": "tsx scripts/mongo/connect.ts",
    "diag:mongo:schema": "tsx scripts/run-with-env.ts scripts/mongo/schema.ts",
    "diag:mongo:content": "tsx scripts/run-with-env.ts scripts/mongo/content.ts",
    "diag:mongo:all": "pnpm diag:mongo:connect && pnpm diag:mongo:schema && pnpm diag:mongo:content",
    "//": "--- [7] DIAGNÓSTICOS DE SERVICIOS ADICIONALES ---",
    "diag:resend": "tsx scripts/resend/diagnostics/all.ts",
    "diag:shopify": "tsx scripts/run-with-env.ts scripts/shopify/diagnostics/all.ts",
    "test:stripe": "tsx scripts/run-with-env.ts scripts/stripe/testing/e2e.ts",
    "//": "--- [8] ORQUESTADOR DE DIAGNÓSTICO TOTAL ---",
    "diag:all": "tsx scripts/orchestrators/test-all-services.ts",
    "//": "--- [9] OPERACIONES DE BASE DE DATOS (SEEDING) ---",
    "db:seed:superuser": "tsx scripts/run-with-env.ts scripts/supabase/seeding/superuser.ts",
    "db:seed:articles": "tsx scripts/run-with-env.ts scripts/supabase/seeding/articles.ts",
    "db:seed:bavi": "tsx scripts/run-with-env.ts scripts/supabase/seeding/bavi.ts",
    "db:seed:razprompts": "tsx scripts/run-with-env.ts scripts/supabase/seeding/razprompts.ts",
    "db:seed:all": "pnpm db:seed:superuser && pnpm db:seed:bavi && pnpm db:seed:razprompts"
